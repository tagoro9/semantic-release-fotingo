{
  "name": "semantic-release-fotingo",
  "version": "1.3.1",
  "description": "Semantic release plugin for fotingo",
  "main": "lib/index.js",
  "repository": "git@github.com:tagoro9/semantic-release-fotingo.git",
  "author": "Victor Mora",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && tsc -p ./tsconfig.build.json && yarn build:fix-paths",
    "build:fix-paths": "tscpaths -p ./tsconfig.build.json -s ./src -o ./lib",
    "clean": "rimraf lib",
    "coverage": "jest --coverage --silent",
    "lint": "eslint ./src --color --ext .ts --ext .tsx",
    "prepack": "yarn clean && yarn build",
    "prepare": "yarn build",
    "test": "jest",
    "watch": "tsc-watch -p ./tsconfig.build.json --onSuccess 'yarn build:fix-paths'"
  },
  "keywords": [
    "fotingo",
    "semantic-release"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "conventional-commits-parser": "^3.2.4",
    "debug": "^4.3.4",
    "fotingo": "^3.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jest/globals": "^28.1.3",
    "@semantic-release/git": "^10.0.1",
    "@types/conventional-commits-parser": "^3.0.2",
    "@types/debug": "^4.1.7",
    "@types/jest": "^28.1.6",
    "@types/semantic-release": "^17.2.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.14.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.0",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.7",
    "tsc-watch": "^5.0.3",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "semantic-release": ">= 19"
  },
  "engines": {
    "node": ">= 14"
  }
}
