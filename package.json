{
  "name": "semantic-release-fotingo",
  "version": "1.2.3",
  "description": "Semantic release plugin for fotingo",
  "main": "lib/index.js",
  "repository": "git@github.com:tagoro9/semantic-release-fotingo.git",
  "author": "Victor Mora",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && tsc -p ./tsconfig.build.json && yarn build:fix-paths",
    "build:fix-paths": "tscpaths -p ./tsconfig.build.json -s ./src -o ./lib",
    "clean": "rimraf lib",
    "coverage": "jest --coverage --silent",
    "lint": "eslint ./src --color --ext .ts --ext .tsx",
    "prepack": "yarn clean && yarn build",
    "prepare": "yarn build",
    "test": "jest",
    "watch": "tsc-watch -p ./tsconfig.build.json --onSuccess 'yarn build:fix-paths'"
  },
  "keywords": [
    "fotingo",
    "semantic-release"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "conventional-commits-parser": "^3.2.1",
    "debug": "^4.3.1",
    "fotingo": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jest/globals": "^26.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/conventional-commits-parser": "^3.0.1",
    "@types/debug": "^4.1.5",
    "@types/semantic-release": "^17.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-typescript-sort-keys": "^1.2.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "husky": "^4.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "semantic-release": "^17.3.9",
    "ts-jest": "^26.5.0",
    "tsc-watch": "^4.2.9",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "semantic-release": "^17.3.9"
  }
}
